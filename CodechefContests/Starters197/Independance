
import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
	Scanner scan = new Scanner(System.in);
	int num = scan.nextInt();
	 int day = 15;
	  int daysLeft = day-num;
	  if(daysLeft>=0){
	      System.out.println(daysLeft);
	  }
	  else{
	      System.out.println(-1);
	  }

	  }
}
